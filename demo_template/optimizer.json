{
    "appDir": "demo_template",
    "dir": "demo",
    "optimize": "uglify2",
    "optimizeCss": "standard",
    "removeCombined": true,
    "findNestedDependencies": true,
    "preserveLicenseComments": false,
    "generateSourceMaps": true,
    "mainConfigFile": "demo_template/js/config.js",
    "modules": [
        {
            "name": "common"
        },
        {
            "name": "index",
            "exclude": ["common"]
        },
        {
            "name": "css",
            "exclude": ["common"]
        },
        {
            "name": "javascript",
            "exclude": ["common"]
        }
    ]
}
